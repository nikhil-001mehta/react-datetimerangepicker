{
  "name": "@technikhil/react-daterangepicker",
  "version": "1.0.9",
  "description": "Simple date and time range picker for react apps",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "serve": "./node_modules/.bin/rimraf ./demo/dist && ./node_modules/.bin/rimraf ./dist && ./node_modules/.bin/concurrently \"./node_modules/.bin/webpack -p --watch\" \"./node_modules/.bin/webpack-serve --config ./webpack.config-demo.js\"",
    "build": "./node_modules/.bin/rimraf ./dist && ./node_modules/.bin/webpack -p",
    "build-demo": "rimraf ./demo/dist && ./node_modules/.bin/webpack -p --config ./webpack.config-demo.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,json,css,jsx,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/testSetup/enzyme.js",
    "testPathIgnorePatterns": [
      "<rootDir>/testSetup",
      "<rootDir>/demo"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/demo/**",
      "!**/dist/**",
      "!**/test/**",
      "!**/webpack.**",
      "!**/testCoverage/**",
      "!**/testSetup/**"
    ],
    "collectCoverage": true,
    "clearMocks": true,
    "coverageDirectory": "testCoverage",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikhil-001mehta/react-datetimerangepicker.git"
  },
  "keywords": [
    "react",
    "datepicker",
    "daterange",
    "daterangepicker",
    "timepicker",
    "input-control"
  ],
  "author": "Nikhil Mehta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nikhil-001mehta/react-datetimerangepicker/issues"
  },
  "homepage": "https://nikhil-001mehta.github.io/react-datetimerangepicker",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "concurrently": "^4.0.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.5.0",
    "eslint-plugin-prettier": "^2.6.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.14",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.2",
    "prop-types": "^15.6.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-serve": "^2.0.2"
  },
  "dependencies": {
    "moment": "^2.22.2",
    "moment-range": "^4.0.1",
    "react": "^16.5.0",
    "react-dom": "^16.5.0"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  }
}

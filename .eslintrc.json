{
  "extends": ["standard", "standard-react", "prettier", "eslint:recommended"],
  "plugins": ["prettier", "react"],
  "rules": {
    "prettier/prettier": "error"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  }
}
